{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"workspaceName": {
			"type": "string",
			"metadata": "Workspace name",
			"defaultValue": "kj-sourcesynapse"
		},
		"kj-sourcesynapse-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'kj-sourcesynapse-WorkspaceDefaultSqlServer'"
		},
		"test trigger change_properties_typeProperties_recurrence_frequency": {
			"type": "string",
			"defaultValue": "Day"
		},
		"test trigger change_properties_typeProperties_recurrence_interval": {
			"type": "int",
			"defaultValue": 1
		},
		"test trigger change_properties_typeProperties_recurrence_startTime": {
			"type": "string",
			"defaultValue": "2021-11-11T15:35:00"
		},
		"test trigger change_properties_typeProperties_recurrence_timeZone": {
			"type": "string",
			"defaultValue": "Eastern Standard Time"
		},
		"test trigger change_properties_typeProperties_recurrence_schedule_minutes": {
			"type": "array",
			"defaultValue": [
				0
			]
		},
		"test trigger change_properties_typeProperties_recurrence_schedule_hours": {
			"type": "array",
			"defaultValue": [
				13
			]
		},
		"test trigger no change_properties_typeProperties_recurrence_frequency": {
			"type": "string",
			"defaultValue": "Day"
		},
		"test trigger no change_properties_typeProperties_recurrence_interval": {
			"type": "int",
			"defaultValue": 1
		},
		"test trigger no change_properties_typeProperties_recurrence_startTime": {
			"type": "string",
			"defaultValue": "2021-11-11T15:34:00"
		},
		"test trigger no change_properties_typeProperties_recurrence_timeZone": {
			"type": "string",
			"defaultValue": "Eastern Standard Time"
		},
		"test trigger no change_properties_typeProperties_recurrence_schedule_minutes": {
			"type": "array",
			"defaultValue": [
				0
			]
		},
		"test trigger no change_properties_typeProperties_recurrence_schedule_hours": {
			"type": "array",
			"defaultValue": [
				13
			]
		}
	},
	"variables": {
		"workspaceId": "[concat('Microsoft.Synapse/workspaces/', parameters('workspaceName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('workspaceName'), '/Pipeline TEST')]",
			"type": "Microsoft.Synapse/workspaces/pipelines",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"activities": [
					{
						"name": "Wait1",
						"type": "Wait",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"waitTimeInSeconds": 1
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/kj-sourcesynapse-WorkspaceDefaultSqlServer')]",
			"type": "Microsoft.Synapse/workspaces/linkedServices",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"parameters": {
					"DBName": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "AzureSqlDW",
				"typeProperties": {
					"connectionString": {
						"type": "SecureString",
						"value": "[parameters('kj-sourcesynapse-WorkspaceDefaultSqlServer_connectionString')]"
					}
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('workspaceName'), '/kj-sourcesynapse-WorkspaceDefaultStorage')]",
			"type": "Microsoft.Synapse/workspaces/linkedServices",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "https://kjastorage.dfs.core.windows.net"
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('workspaceName'), '/test trigger change')]",
			"type": "Microsoft.Synapse/workspaces/triggers",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "Pipeline TEST",
							"type": "PipelineReference"
						},
						"parameters": {}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "[parameters('test trigger change_properties_typeProperties_recurrence_frequency')]",
						"interval": "[parameters('test trigger change_properties_typeProperties_recurrence_interval')]",
						"startTime": "[parameters('test trigger change_properties_typeProperties_recurrence_startTime')]",
						"timeZone": "[parameters('test trigger change_properties_typeProperties_recurrence_timeZone')]",
						"schedule": {
							"minutes": "[parameters('test trigger change_properties_typeProperties_recurrence_schedule_minutes')]",
							"hours": "[parameters('test trigger change_properties_typeProperties_recurrence_schedule_hours')]"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/pipelines/Pipeline TEST')]"
			]
		},
		{
			"name": "[concat(parameters('workspaceName'), '/test trigger no change')]",
			"type": "Microsoft.Synapse/workspaces/triggers",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "[parameters('test trigger no change_properties_typeProperties_recurrence_frequency')]",
						"interval": "[parameters('test trigger no change_properties_typeProperties_recurrence_interval')]",
						"startTime": "[parameters('test trigger no change_properties_typeProperties_recurrence_startTime')]",
						"timeZone": "[parameters('test trigger no change_properties_typeProperties_recurrence_timeZone')]",
						"schedule": {
							"minutes": "[parameters('test trigger no change_properties_typeProperties_recurrence_schedule_minutes')]",
							"hours": "[parameters('test trigger no change_properties_typeProperties_recurrence_schedule_hours')]"
						}
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/AutoResolveIntegrationRuntime')]",
			"type": "Microsoft.Synapse/workspaces/integrationRuntimes",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 0
						}
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/WorkspaceSystemIdentity')]",
			"type": "Microsoft.Synapse/workspaces/credentials",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"type": "ManagedIdentity",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/kjSourceDedicated')]",
			"type": "Microsoft.Synapse/workspaces/sqlPools",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"collation": "SQL_Latin1_General_CP1_CI_AS",
				"maxSizeBytes": 263882790666240,
				"restorePointInTime": "0001-01-01T00:00:00",
				"annotations": []
			},
			"dependsOn": [],
			"location": "eastus2"
		}
	]
}